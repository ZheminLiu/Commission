<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="strutsBean" extends="struts-default" namespace="/">
		<action name="loginAction" class="userAction" method="login">
			<result name="salesLogSuccess">/salesmanIndex.jsp</result>
			<result name="gunLogSuccess">/gunsmithIndex.jsp</result>
			<result name="logFailed">/WEB-INF/loginFail.html</result>
		</action>
		<action name="commissionAction" class="userAction" method="searchMonthReport">
			<result name="success">/curMonthlyReport.jsp</result>
			<result name="fail">/curMonthlyReport.jsp</result>
		</action>
		<action name="historyAction" class="userAction" method="searchHistoryReport">
			<result name="success">/historyMonthlyReport.jsp</result>
			<result name="fail">/historyMonthlyReport.jsp</result>
		</action>
		<action name="saleAction" class="saleAction" method="saveSalesrecord">
			<result name="success">/salesmanIndex.jsp</result>
			<result name="fail">/salesmanIndex.jsp</result>
		</action>
	</package>
</struts>    
